# Server Configuration
PORT=5000
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your-supabase-project-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Redis Configuration (for Bull queues)
REDIS_URL=redis://localhost:6379

# File Upload
FILE_UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# n8n Integration
N8N_BASE_URL=http://localhost:5678
N8N_API_KEY=your-n8n-api-key-generate-from-n8n-ui
N8N_WEBHOOK_BASE_URL=http://localhost:5678/webhook

# n8n Workflow IDs (copy from n8n after importing workflows)
N8N_SALES_AGENT_WORKFLOW_ID=
N8N_TECHNICAL_AGENT_WORKFLOW_ID=
N8N_PRICING_AGENT_WORKFLOW_ID=
N8N_REPORT_AGENT_WORKFLOW_ID=
N8N_MASTER_AGENT_WORKFLOW_ID=

